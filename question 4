// 10)Calculate Simple Interest
var p = 1000;

var n = 2;

var r = 4;

var si = ((p*n*r)/100);

console.log(si);

// 11)Calculate area of an equilateral triangle
var a = 20;

var b = ((Math.sqrt(3)/4)*a*a);

console.log(b);

// 12)Area Of Isosceles Triangle
var b = 10;

var h = 10;

var A = ((b*h)/2);

console.log(A);

// 13)Volume Of Sphere
var r = 2;

var sphere = ((4/3)*(22/7)*r*r*r);

console.log(sphere);

// 14)Volume Of Prism
