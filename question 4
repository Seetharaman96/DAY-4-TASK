// Calculate electricity bill?
// For example, a consumer consumes 100 watts per hour daily for one month. Calculate the
// total energy bill of that consumer if per unit rate is 10?
var watts = 100;

var perUnitRate = 10;

var kw = (100/1000);

var time = (1*30);

var kwPerMonth = (kw*time);

var costOfElecticity = (kwPerMonth * perUnitRate);

console.log(costOfElecticity);

// Program To Calculate CGPA
var tamil = 9.5;

var english = 8;

var maths = 9;

var science = 7;

var social = 9.5;

var cgpa = ((tamil+english+maths+science+social)/5);

console.log(cgpa);


