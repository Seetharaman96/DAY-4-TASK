console.log(si);

// Calculate area of an equilateral triangle
var a = 20;

var b = ((Math.sqrt(3)/4)*a*a);

console.log(b);

// Area Of Isosceles Triangle
var b = 10;

var h = 10;

var A = ((b*h)/2);

console.log(A);

// Volume Of Sphere
var r = 2;

var sphere = ((4/3)*(22/7)*r*r*r);

console.log(sphere);

